{
  "name": "@cdt/frontend",
  "version": "4.51.3",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "node server/server.js",
    "lint-js": "eslint src pages __tests__ __mocks__",
    "lint-css": "stylelint 'src/**/*.js' 'src/**/*.css' 'pages/**/*.js'",
    "lint": "yarn lint-js && yarn lint-css",
    "precommit": "lint-staged",
    "prepush": "yarn test --bail --changedSince=master",
    "start": "NODE_ENV=production node server/server.js",
    "build": "next build",
    "analyze-bundles": "ANALYZE=true next build",
    "test": "NODE_ENV=test jest",
    "generate-sitemap": "node -r esm ./scripts/generate-sitemap"
  },
  "dependencies": {
    "@cdt/data": "^4.49.0",
    "@cdt/data...prime-precarite": "^4.46.0",
    "@cdt/data...simulateurs": "^4.46.0",
    "@cdt/data...tools": "^4.49.0",
    "@matejmazur/react-katex": "^3.1.3",
    "@next/bundle-analyzer": "^10.0.9",
    "@sentry/browser": "^6.2.2",
    "@sentry/node": "^6.2.2",
    "@socialgouv/cdtn-logger": "^1.1.2",
    "@socialgouv/cdtn-slugify": "^4.45.0",
    "@socialgouv/cdtn-sources": "^4.45.0",
    "@socialgouv/cdtn-ui": "^4.51.0",
    "@socialgouv/fiches-travail-data": "^4.162.0",
    "@socialgouv/modeles-social": "^4.51.0",
    "@socialgouv/react-fiche-service-public": "^4.51.0",
    "@socialgouv/reference-article": "^1.0.1",
    "@ungap/url-search-params": "^0.2.2",
    "@zeit/next-source-maps": "^0.0.3",
    "date-fns": "^2.19.0",
    "debounce-promise": "^3.1.2",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "js-cookie": "2.2.1",
    "katex": "^0.12.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "koa-router": "^10.0.0",
    "lit-element": "2.4.0",
    "memoizee": "^0.4.15",
    "next": "10.2.1",
    "next-transpile-modules": "^6.3.0",
    "p-debounce": "^2.1.0",
    "publicodes": "^1.0.0-beta.15",
    "react": "^17.0.1",
    "react-autosuggest": "10.1.0",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-final-form-listeners": "^1.0.3",
    "react-svg-spinner": "^1.0.4",
    "react-uid": "^2.3.1",
    "rehype-parse": "^7.0.1",
    "rehype-raw": "^5.1.0",
    "rehype-react": "^6.2.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.2.4",
    "unified": "^9.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@socialgouv/eslint-config-react": "^1.59.0",
    "@testing-library/react": "^12.0.0",
    "@types/debounce-promise": "^3.1.3",
    "@types/memoizee": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.21.0",
    "esm": "^3.2.25",
    "ignore-loader": "^0.1.2",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.3",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.1.3"
  },
  "lint-staged": {
    "{pages,src}/**/*.{css,js}": [
      "yarn lint-css"
    ],
    "{__tests__,pages,server,src}/**/*.js": [
      "yarn lint-js --fix"
    ],
    "{__mocks__,test}/**/*.js": [
      "yarn lint-js --fix"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*?(*.)+(test|spec).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "!src/**/*mock.js",
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  }
}
